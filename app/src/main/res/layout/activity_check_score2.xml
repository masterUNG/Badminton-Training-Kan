<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kbu.sineepun.kanpengnet.badmintontraining.CheckScore2"
    android:transitionGroup="false">

    <RelativeLayout tools:context="kbu.sineepun.kanpengnet.badmintontraining.CheckScore"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="false"
            android:layout_alignParentBottom="true"
            android:id="@+id/linearLayout"
            android:background="#003700" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView21"
                android:layout_weight="1"
                android:src="@drawable/manicon" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView22"
                android:layout_weight="1"
                android:src="@drawable/man2icon" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView23"
                android:layout_weight="1"
                android:src="@drawable/undo" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView24"
                android:layout_weight="1"
                android:src="@drawable/reset" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/linearLayout"
            android:background="@drawable/bg_score_02"
            android:layout_alignWithParentIfMissing="false">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView25"
                android:layout_centerInParent="true" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView26"
                    android:layout_weight="1" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView27"
                    android:layout_weight="1" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:id="@+id/textView22"
                android:textSize="@dimen/h1"
                android:textStyle="bold"
                android:textColor="@color/text"
                android:layout_centerHorizontal="true"
                android:layout_above="@+id/imageView25"
                android:layout_marginBottom="15dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:id="@+id/textView23"
                android:textSize="@dimen/h1"
                android:textStyle="bold"
                android:textColor="@color/text"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="15dp"
                android:layout_below="@+id/imageView25" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView24"
                android:textSize="@dimen/h1"
                android:textStyle="bold"
                android:textColor="@color/text"
                android:layout_above="@+id/imageView25"
                android:layout_marginBottom="120dp"
                android:layout_marginLeft="80dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView26"
                android:textSize="@dimen/h1"
                android:textStyle="bold"
                android:textColor="@color/text"
                android:layout_above="@+id/imageView25"
                android:layout_marginBottom="120dp"
                android:layout_marginLeft="220dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView25"
                android:textSize="@dimen/h1"
                android:textStyle="bold"
                android:textColor="@color/text"
                android:layout_marginTop="130dp"
                android:layout_below="@+id/imageView25"
                android:layout_alignParentStart="false"
                android:layout_marginLeft="80dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                android:textSize="@dimen/h1"
                android:textStyle="bold"
                android:textColor="@color/text"
                android:layout_marginTop="130dp"
                android:layout_below="@+id/imageView25"
                android:layout_alignParentStart="false"
                android:layout_marginLeft="220dp"
                android:layout_alignParentEnd="false" />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
